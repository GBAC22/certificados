{
  "name": "sistema-certificados",
  "version": "1.0.0",
  "description": "Sistema completo de certificados blockchain",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd backend && npm run dev",
    "dev:frontend": "cd frontend && npm run dev",
    "install:all": "npm install && cd backend && npm install && cd ../frontend && npm install && cd ../blockchain && npm install",
    "init-db": "cd backend && npm run init-db"
  },
  "keywords": ["blockchain", "certificados", "fullstack"],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
